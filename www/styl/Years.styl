.years
  width 100%
  height 600px
  .year
    float left
    width 21px
    height @height
    position relative
    overflow hidden
    .death
      height 5px
      width 5px
      position absolute
      border-right 1px solid black
      border-top 1px solid black
      &:nth-child(4n)
        border-right 0px none
