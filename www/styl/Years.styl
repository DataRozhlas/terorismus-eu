.years
  width 100%
  height 600px
  position relative
  .year
    float left
    width 21px
    height @height
    position relative
    .title
      position absolute
      bottom -25px
      font-size 12px
      position absolute
      left 50%
      width 40px
      margin-left -20px
      text-align center

  .death
    height 3px
    width 3px
    border-right 1px solid black
    border-top 1px solid black
    position absolute
    &.last-col
      width 2px
      border-right 0px none
    &.active
      opacity 0.5
